    "keywords": [
        "laravel",
        "cms",
        "admin-minton"
    ],
    "authors": [
        {
            "name": "Mark Villudo"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-0": {
            "MarkVilludo\\AdminMinton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarkVilludo\\AdminMinton\\ServiceProvider"
            ],
            "aliases": {
                "AdminMinton": "MarkVilludo\\AdminMinton\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
